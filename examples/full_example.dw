// Struct
struct Person {
	name = "unknown"
	age = 0

	func get_age() {
		return self::age
	}

	func who() {
		print("Person ", self::name, " is ", self::age, " years old")
	}
}

func struct_examp() {
	var people = []
	people += [Person{}]
	people += [Person{ name="Krim", age=23 }]

	for ref person: people {
		person::who()
	}
}
