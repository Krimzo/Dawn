// Logic
func logic_examp() {
	let a = (5 + 1.0) * 2.
	if a < 10.0 {
		print("res = " + string(a))
	}
	elif a <= 15.0 {
		print("it's less or equal to 15")
	}
	else {
		print("it's more than 15")
	}
}

// Switch
func switch_examp() {
	let value = 16
	switch value {
		case 1, 2, 3 {
			print("it's 3")
		}
		case 14 {
			print("it's 14")
		}
		default {
			print("it's something that im not aware of")
		}
	}
}

// For
func for_examp() {
	for ref i: 0>>10 {
		print(i)
	}
	for ref c: "some text" {
		print(c)
	}
	for ref value: int[5] {
		print(value)
	}
}

// While
func while_examp() {
	var counter = 0
	while counter < 10 {
		if counter == 6 {
			break
		}
		counter += 1
	}
}

// Loop
func loop_examp() {
