class Float2 {
	pub float x, y;

	Float2() {
		x = 0.0;
		y = 0.0;
	}
	Float2(float a) {
		x = a;
		y = a;
	}
	Float2(float x, float y) {
		self->x = x;
		self->y = y;
	}

	add(Float2 obj) -> Float2 {
		return Float2(x + obj->x, y + obj->y);
	}
	sub(Float2 obj) -> Float2 {
		return Float2(x - obj->x, y - obj->y);
	}
}

Float2 vector = Float2(1.0, 2.0);


