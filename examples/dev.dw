func main()
{
    var a = Person{age=5}
    var b = Person{age=7}
    a->who()
    b->who()
    var c = a + b
    print(c)

    var animals = Animal[c->age]
    print(animals)
}

enum Animal
{
    none
    dog = "dog"
    cat = 'c'
}

struct Person {
    age = 0

    func who() {
        print("age ", self->age)
    }

    oper +(ref other) {
        return Person{age=self->age + other->age}
    }
}
