func main() {
	var data = [1, 2, 3]
	for ref i : data {
		i *= 2
	}
	print(data)

	var p = Person{ name: "Krim", age: 23 }
	ref pp = p
	ref ppp = pp
	ref pppp = ppp
	alter(pppp->age)
	print(p)
}

func alter(ref val) {
	val *= 5
}

struct Person {
	name = "wasd",
	age = 2 * 2,
	
	func who() {
		print(self->name, " is ", self->age, " years old")
	}
}
