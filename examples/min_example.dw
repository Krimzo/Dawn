func main() {
	let upper_limit = 10
	let half = upper_limit / 2

	let data = [rand_int(upper_limit), rand_int(upper_limit), rand_int(upper_limit), rand_int(upper_limit)]
	for let value : data {
		verify(value, half)
	}
}

func verify(ref p, let check) {
	if p > check {
		print(value, " is larger than ", check)
	}
	elif p == check {
		print(value, " is same as ", check)
	}
	else {
		print(value, " is smaller than ", check)
	}
}

