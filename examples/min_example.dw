func main() {
	let a = [1, 2, 3]
	for ref i : a {
		i *= 2
	}
	print(a)
	
	let person = Person{ name: "Krim", age: 23 }
	alter(person->age)
	person->who()
}

func alter(ref val) {
	val *= 5
}

struct Person {
	name = "wasd",
	age = 2 * 2,
	
	func who() {
		print(self->name, " is ", self->age, " years old")
	}
}
